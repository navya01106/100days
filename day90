//Q140: Define a struct with enum Gender and print person's gender.

/*
Sample Test Cases:
Input 1:
Gender=MALE
Output 1:
Male

*/
#include <stdio.h>
#include <string.h>

enum Gender {
    MALE,
    FEMALE,
    OTHER
};

struct Person {
    enum Gender g;
};

void printGender(struct Person p) {
    if (p.g == MALE) {
        printf("Male\n");
    } else if (p.g == FEMALE) {
        printf("Female\n");
    } else if (p.g == OTHER) {
        printf("Other\n");
    }
}

int main() {
    char input[20];
    struct Person p;
    
    scanf("Gender=%s", input);
    
    if (strcmp(input, "MALE") == 0) {
        p.g = MALE;
    } else if (strcmp(input, "FEMALE") == 0) {
        p.g = FEMALE;
    } else if (strcmp(input, "OTHER") == 0) {
        p.g = OTHER;
    }
    
    printGender(p);
    
    return 0;
}
